{
  "manifest_version": 2,
  "name": "Ad Value React",
  "description": "Lets figure out how much you cost this website",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "*://*.google.com/",
    "*://*.doubleclick.net/"
  ]
}
